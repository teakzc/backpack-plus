{
	"name": "jest test",
	"globIgnorePaths": [
		"**/package.json",
		"**/tsconfig.json"
	],
	"tree": {
		"$className": "DataModel",
		"ServerScriptService": {
			"$className": "ServerScriptService",
			"TS": {
				"$path": "out/tests/server"
			},
			"JestTests": {
				"$path": "out/tests/jest"
			}
		},
		"ReplicatedStorage": {
			"$className": "ReplicatedStorage",
            "rbxts_include": {
                "$path": "include",
                "node_modules": {
                    "$className": "Folder",
                    "@rbxts": {
                        "$path": "node_modules/@rbxts"
                    },
                    "@rbxts-js": {
                        "$path": "node_modules/@rbxts-js"
                    }
                }
            },
			"Package": {
				"$path": "out/lib"
			}
		},
		"StarterPlayer": {
			"$className": "StarterPlayer",
			"StarterPlayerScripts": {
				"$className": "StarterPlayerScripts",
				"TS": {
					"$path": "out/tests/client"
				}
			}
		}
	}
}